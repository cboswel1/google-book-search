{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "A book searching utility using React",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "start:server": "nodemon server/server.js",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cboswel1/google-book-search.git"
  },
  "author": "Christian Boswell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cboswel1/google-book-search/issues"
  },
  "homepage": "https://github.com/cboswel1/google-book-search#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "express": "^4.17.1",
    "mdbreact": "^5.0.1",
    "mongoose": "^5.11.8",
    "nodemon": "^2.0.6",
    "path": "^0.12.7"
  }
}
